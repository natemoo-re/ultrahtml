{
  "name": "ultrahtml",
  "type": "module",
  "version": "1.0.0-next.1",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/natemoo-re/ultrahtml"
  },
  "bugs": {
    "url": "https://github.com/natemoo-re/ultrahtml/issues"
  },
  "homepage": "https://github.com/natemoo-re/ultrahtml#README",
  "scripts": {
    "build": "node scripts/build.js && tsc -p .",
    "lint": "prettier \"**/*.{js,ts,md}\"",
    "dev": "vitest",
    "test": "vitest run"
  },
  "files": [
    "selector.d.ts",
    "transform.d.ts",
    "jsx-runtime.d.ts",
    "transformers",
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./selector": {
      "types": "./dist/selector.d.ts",
      "import": "./dist/selector.js"
    },
    "./transformers/*": {
      "types": "./dist/transformers/*.d.ts",
      "import": "./dist/transformers/*.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "import": "./dist/jsx-runtime/index.js"
    }
  },
  "keywords": [
    "html",
    "template",
    "sanitize"
  ],
  "author": {
    "name": "Nate Moore",
    "email": "nate@natemoo.re",
    "url": "https://twitter.com/n_moore"
  },
  "license": "MIT",
  "volta": {
    "node": "18.7.0"
  },
  "packageManager": "pnpm@7.6.0",
  "devDependencies": {
    "@changesets/cli": "^2.18.1",
    "@types/stylis": "^4.0.2",
    "chalk": "^5.1.2",
    "esbuild": "^0.14.51",
    "globby": "^13.1.2",
    "gzip-size": "^7.0.0",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "parsel-js": "^1.0.2",
    "prettier": "^2.5.1",
    "pretty-bytes": "^6.0.0",
    "stylis": "^4.1.2",
    "typescript": "^4.7.4",
    "vitest": "^0.20.2"
  }
}
